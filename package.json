{
  "name": "waveresearch-interface",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "build:with-env": "echo 'Building with environment variables...' && vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "proxy": "node proxy-server.js",
    "backend": "cd backend && npm start",
    "backend:dev": "cd backend && npm run dev",
    "backend:install": "cd backend && npm install",
    "dev:full": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "dev:legacy": "concurrently \"npm run proxy\" \"npm run dev\"",
    "test": "node scripts/test-runner.js",
    "test:verbose": "node scripts/test-runner.js --verbose",
    "test:basic": "node scripts/test-system.js",
    "test:health": "node scripts/health-checks.js",
    "test:integration": "node scripts/integration-tests.js",
    "test:deployment": "node scripts/deployment-validator.js",
    "test:quick": "node scripts/test-runner.js --skip-integration --skip-deployment",
    "test:full": "node scripts/test-runner.js --verbose --output-file test-results.log"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "lucide-react": "^0.544.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.6.2",
    "vite": "^5.4.20"
  },
  "description": "**Estado del Proyecto**: ✅ Integración básica n8n completada - En desarrollo activo   **Versión Actual**: 1.1.0   **Última Actualización**: Enero 2025",
  "main": "vite.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aguimoda/WaveResearchFront.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aguimoda/WaveResearchFront/issues"
  },
  "homepage": "https://github.com/Aguimoda/WaveResearchFront#readme"
}
